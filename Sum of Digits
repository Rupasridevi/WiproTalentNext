import java.util.*;
class Number{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int fin = 0,rem;
        System.out.print("Enter the number");
        int n = sc.nextInt();
        while(n>0)
        {
            rem = n%10;
            fin = fin+rem;
            n = n/10;
        }
        System.out.println("sum of digits"+fin);

    }
}
